<% content_for :head do %>
  <%= javascript_include_tag "group" %>
  <%= stylesheet_link_tag "groups" %>
  <script type="text/javascript">
    <% if  !@group.lon.nil? and !@group.lat.nil? -%>
        OSM.group_home = <%= { :lat => @group.lat, :lon => @group.lon }.to_json.html_safe %>;
    <% end -%>
  </script>
<% end %>