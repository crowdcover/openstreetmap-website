<%  Tile.find_each do | tile | %>
  L.Site.<%= tile.name.capitalize %> = L.OSM.TileLayer.extend({
  options: {
    url: '<%= tile.url %>',
    attribution: "<%= tile.attribution %>",
    name: "<%= tile.name %>",
    code: "<%= tile.code %>",
    keyid: "<%= tile.keyid %>",
    subdomains: "<%= tile.subdomains %>",
    base_layer: "<%= tile.base_layer %>",
    description: "<%= tile.description %>"
  }
});
<% end %>
